DB_HOST='{{ env_db_host }}'
DB_NAME='{{ env_db_name }}'
DB_PASSWORD='{{ env_db_pass }}'
DB_USER='{{ env_db_user }}'
# DB_DEBUG:
#   TRUE: fail on database errors on migration, display to user
#   FALSE: continue regardless of errors
DB_DEBUG=FALSE

S3_BUCKET='{{ s3_bucket }}'
S3_PREFIX='{{ s3_prefix }}'

ENCRYPTION_KEY='{{ env_encryption_key }}'
CONTENT_PROTOCOL='{{ env_content_protocol}}'
PROJECT_SHARED_PATH='{{ project_shared_path }}'
PRE_APPROVED_ADMINS='{{ codeigniter_pre_approved_admins | join(",") }}'
DEFAULT_HOST='{{ default_host }}'
ENVIRONMENT='{{ web_app_env }}'
