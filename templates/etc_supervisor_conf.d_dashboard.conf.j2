[program:dashboard-campaign-status-download]
command=timeout --preserve-status 23h /usr/bin/php {{ current_source_symlink }}/public/index.php campaign status omb-monitored download
stdout_logfile=/var/log/dashboard/campaign-status-download.log
redirect_stderr=true
autorestart=false
autostart=false
user=ubuntu

[program:dashboard-campaign-status-full-scan]
command=timeout --preserve-status 23h /usr/bin/php {{ current_source_symlink }}/public/index.php campaign status omb-monitored full-scan
stdout_logfile=/var/log/dashboard/campaign-status-full-scan.log
redirect_stderr=true
autorestart=false
autostart=false
user=ubuntu

[program:dashboard-campaign-status-long-running]
command=/usr/bin/php {{ current_source_symlink }}/public/index.php campaign status long-running full-scan
stdout_logfile=/var/log/dashboard/campaign-status-full-scan.log
redirect_stderr=true
autorestart=false
autostart=true
user=ubuntu
